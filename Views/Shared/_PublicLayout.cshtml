<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="@Url.Content("~/content/images/pgicon.ico")">
    <script type="text/javascript">
        window.MyWebAppBasePath = '@Url.Content("~")';
        window.LoginPanelPath = '@Url.Action("LoginPanel", "Login")';

        if (typeof console === "undefined") {
            console = {};
            console.log = function () { };
        }

        if (!Array.prototype.indexOf) {
            Array.prototype.indexOf = function (obj, start) {
                for (var i = (start || 0), j = this.length; i < j; i++) {
                    if (this[i] === obj) { return i; }
                }
                return -1;
            }
        }
    </script>
    @Styles.Render("~/Content/myfiles/css")
    @Styles.Render("~/Content/themes/base/css")
    @Styles.Render("~/Content/autocomplete1")
    @*--------------JQuery Main Files@*--------------*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @*@*--------------Shared Plugins--------------*@
    @Scripts.Render("~/bundles/sharedplugins")
    @Scripts.Render("~/bundles/autocomplete")
    @*@*--------------Global/Common@*--------------*@
    @Scripts.Render("~/bundles/common")
    @*----MyWebApp Core Javascript files-----*@
    @Scripts.Render("~/bundles/mywebappmain")
    @*----Hi-Charts Javascript files-----*@
    @Scripts.Render("~/bundles/hicharts")
    @*----Case Javascript files-----*@
    @Scripts.Render("~/bundles/base")
    <script src="~/Scripts/mywebapp/core/mywebapp.globals.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-plugins/Template/handlebars.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-plugins/PineNotification/jquery.pnotify.js")"></script>
    <link href="@Url.Content("~/Content/style/PineNotifications/jquery.pnotify.default.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/style/PineNotifications/icons/jquery.pnotify.default.icons.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/style/PineNotifications/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/tooltipster/tooltipster.css")" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-plugins/tooltipster/jquery.tooltipster.min.js")"></script>

    <!--Own Template Scripts-->
    <!-- bootstrap & fontawesome -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/font-awesome/4.5.0/css/font-awesome.min.css" />

    <!-- page specific plugin styles -->
    <!-- text fonts -->
    <link rel="stylesheet" href="~/Content/css/fonts.googleapis.com.css" />

    <!-- ace styles -->
    <link rel="stylesheet" href="~/Content/css/ace.min.css" class="ace-main-stylesheet" id="main-ace-style" />

    <!--[if lte IE 9]>
        <link rel="stylesheet" href="assets/css/ace-part2.min.css" class="ace-main-stylesheet" />
    <![endif]-->
    <link rel="stylesheet" href="~/Content/css/ace-skins.min.css" />
    <link rel="stylesheet" href="~/Content/css/ace-rtl.min.css" />
    <link rel="stylesheet" href="~/Content/css/extraCss.css" />
    <!--[if lte IE 9]>
      <link rel="stylesheet" href="assets/css/ace-ie.min.css" />
    <![endif]-->
    <!-- inline styles related to this page -->
    <!-- ace settings handler -->
    <script src="~/Scripts/templateScripts/ace-extra.min.js"></script>

    <!--***From the Footer File, Review them Later and remove unnecessary File :P ***-->
