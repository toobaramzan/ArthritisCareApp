      <script src="assets/js/excanvas.min.js"></script>
    <![endif]-->
    <script src="~/Scripts/templateScripts/jquery-ui.custom.min.js"></script>
    <script src="~/Scripts/templateScripts/jquery.ui.touch-punch.min.js"></script>
    <script src="~/Scripts/templateScripts/jquery.easypiechart.min.js"></script>
