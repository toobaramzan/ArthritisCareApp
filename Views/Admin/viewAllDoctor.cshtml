
@section Header
{
    <script type="text/javascript" src="@Url.Content("~/Scripts/mywebapp/AdminScripts/mywebapp.ui.docSearch.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/mywebapp/searchAdditionlFunction.js")"></script>
}

@section scripts
{
